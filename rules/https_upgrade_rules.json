[
  {
    "id": 200,
    "priority": 1,
    "action": {
      "type": "redirect",
      "redirect": {
        "regexSubstitution": "https://\\1"
      }
    },
    "condition": {
      "regexFilter": "^http://([^/]+)/.*",
      "resourceTypes": ["main_frame", "sub_frame"]
    }
  },
  {
    "id": 201,
    "priority": 2,
    "action": {
      "type": "upgradeScheme"
    },
    "condition": {
      "urlFilter": "http://*",
      "resourceTypes": ["main_frame", "sub_frame", "stylesheet", "script", "image", "font", "object", "xmlhttprequest", "ping", "csp_report", "media", "websocket", "other"]
    }
  }
]
